<%= form_for(@brew) do |f| %>
    <%= f.label :blend_name %>
    <%= f.text_field :blend_name %><br>
    <%= f.label :origin %>
    <%= f.text_field :origin %><br>
    <%= f.label :notes %>
    <%= f.text_field :notes %><br>
    <%= f.label :price %>
    <%= f.text_field :price %><br>
    <%= f.label :strength %>
    <%= f.text_field :strength %><br>
    <%= f.submit %>
<% end %>